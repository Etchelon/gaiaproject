{
	"name": "svelte-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"rbuild": "rollup -c",
		"rdev": "rollup -c -w",
		"dev": "vite",
		"build": "tsc --noEmit && vite build",
		"start": "sirv public --no-clear",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"android": "pnpm build && pnpm cap run android",
		"android:copy": "cap copy android",
		"android:dev": "ionic cap run android --livereload --external --port=3000 --ssl",
		"android:open": "cap open android",
		"android:sync": "cap sync android",
		"ionic:serve": "vite"
	},
	"dependencies": {
		"@auth0/auth0-spa-js": "^1.22.0",
		"@capacitor/android": "^3.5.1",
		"@capacitor/app": "^1.1.1",
		"@capacitor/browser": "^1.0.7",
		"@capacitor/core": "^3.5.1",
		"@ionic/core": "^6.1.7",
		"@microsoft/signalr": "^6.0.5",
		"ionicons": "6.0.1",
		"lodash": "^4.17.21",
		"luxon": "^2.4.0",
		"rxjs": "^7.5.5",
		"svelte-spa-router": "^3.2.0"
	},
	"devDependencies": {
		"@capacitor/cli": "^3.5.1",
		"@ionic/cli": "^6.19.1",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.0.0",
		"@sveltejs/vite-plugin-svelte": "1.0.0-next.44",
		"@tsconfig/svelte": "^2.0.0",
		"@types/lodash": "^4.14.182",
		"@types/luxon": "^2.3.2",
		"autoprefixer": "^10.4.7",
		"postcss": "^8.4.14",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sass": "1.52.1",
		"svelte": "^3.0.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vite": "^2.9.0"
	},
	"type": "module"
}